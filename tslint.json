{
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore"],
      "prefer-const": [true, {"destructuring": "all"}],
      "max-line-length": false,
      "no-console": false,
      "no-var-requires": false,
      "no-default-export": true,
      "import-spacing": true,
      "semicolon": [true, "never"]
    },
    "rulesDirectory": []
}
